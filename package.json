{
  "name": "x-tools",
  "version": "0.0.0-alpha.2",
  "description": "typescript 编写",
  "main": "./dist",
  "types": "./src",
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
